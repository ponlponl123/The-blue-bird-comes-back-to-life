{
    "name": "Return A Blue Bird",
    "version": "1.0",
    "description": "For Elon Mask",
    "manifest_version": 3,
    "action": {
      "default_title": "Return A Blue Bird",
      "default_popup": "popups/popup.html",
      "default_icon": {
        "32": "icons/32.png",
        "72": "icons/72.png",
        "128": "icons/128.png",
        "512": "icons/512.png"
      }
    },
    "icons": {
      "32": "icons/32.png",
      "72": "icons/72.png",
      "128": "icons/128.png",
      "512": "icons/512.png"
    },
    "content_scripts": [
        {
            "matches": ["https://x.com/*", "https://twitter.com/*"],
            "css": ["main.css"],
            "js": ["main.js"]
        }
    ]
  }